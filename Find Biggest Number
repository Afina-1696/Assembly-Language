
.MODEL SMALL
.STACK 100H
.DATA
    NUM1 DB ?
    NUM2 DB ?
    NUM3 DB ?
    MSG1 DB 10,13, "ENTER THE NUM1: $"
    MSG2 DB 10,13, "ENTER THR NUM2: $"
    MSG3 DB 10,13, "ENTER THE NUM3: $"
    MSG4 DB 10,13, "THE BIGGEST NUMBER IS: $"
    
.CODE
MAIN PROC
    MOV AX, @DATA
    MOV DS, AX
    
    MOV AH, 9
    MOV DL, OFFSET MSG1
    INT 21H
    
    MOV AH, 1
    INT 21H
    MOV NUM1, AL
    
        
    MOV AH, 9
    MOV DL, OFFSET MSG2
    INT 21H
    
    MOV AH, 1
    INT 21H
    MOV NUM2, AL
    
        
    MOV AH, 9
    MOV DL, OFFSET MSG3
    INT 21H
    
    MOV AH, 1
    INT 21H
    MOV NUM3, AL
    
    MOV BL, NUM1
    CMP BL, NUM2
    JNG A
    CMP BL, NUM3
    JNG B
    MOV AH, 9
    MOV DL, OFFSET MSG4
    INT 21H
    
    MOV AH, 2 
    MOV DL, BL
    INT 21H
    
    JMP EXIT
    
    A:
    MOV BL, NUM2
    CMP BL, NUM3
    JNG B
    MOV BL, NUM2
    MOV AH, 9
    MOV DL, OFFSET MSG4
    INT 21H
    
    MOV AH, 2 
    MOV DL, BL
    INT 21H
    
    JMP EXIT

    
    B:
    MOV BL, NUM3
    MOV AH, 9
    MOV DL, OFFSET MSG4
    INT 21H
    
    MOV AH, 2 
    MOV DL, BL
    INT 21H
    
                
    EXIT:
    MOV AH, 4CH
    INT 21H
    MAIN ENDP
END MAIN
    
